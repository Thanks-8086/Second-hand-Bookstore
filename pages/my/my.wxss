page {
  background: #efefef;
  font-size: 30rpx;
  color: #3f3f3f;
}

image {
  width: 32rpx;
  height: 32rpx;
}

text {
  margin: 0 10rpx;
}

.container {
  background-color: white;
  padding-top: 30rpx;
  padding-bottom: 0rpx;
}

.userinfo {
  width: 100%;
  padding-bottom: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-content: center;
  border-bottom: 2rpx #efefef solid;
}

.userinfo-avatar {
  height: 100rpx;
  border-radius: 50%;
  background-size: cover;
  background-color: white;
  margin-left: 100rpx;
  margin-right: 30rpx;
}

.userinfo-avatar[size] {
  width: 100rpx;
}

.userinfo-avatar:after {
  border: none;
}

.usrname {
  height: 50rpx;
  margin: 25rpx 10rpx;
}

.bankuai1 {
  width: 100%;
  padding-top: 20rpx;
  border-bottom: 15rpx #efefef solid;
}

.rowTag {
  margin: 20rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-content: center;
}

.rowtags {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center;
}

.rowtags image {
  width: 50rpx;
  height: 50rpx;
  margin: 0 20rpx;
}

.rowtags text {
  margin: 15rpx 0;
  font-size: 27rpx;
}

.tiaolie {
  padding: 25rpx 0 25rpx 40rpx;
  border-bottom: 2rpx #efefef solid;
}

.bankuai2 {
  width: 100%;
  margin-top: 20rpx;
}
